{
  "name": "nftin",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "types-check": "tsc --project tsconfig.json --noEmit --noEmitHelpers --noErrorTruncation --strict",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx,.json",
    "lint:fix": "yarn lint --fix",
    "lint:ci": "yarn lint --quiet",
    "precommit": "lint-staged",
    "release": "np --no-publish --no-tests --no-release-draft",
    "postinstall": "node_modules/.bin/rn-nodeify --install crypto,assert,stream,events --yarn"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "./configs/eslint.config"
    ]
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "@types/styled-components": "^5.1.26",
    "@types/styled-components-react-native": "^5.1.3",
    "@walletconnect/react-native-dapp": "^1.8.0",
    "@walletconnect/web3-provider": "^1.8.0",
    "base-64": "^1.0.0",
    "big-integer": "^1.6.51",
    "effector": "^22.3.0",
    "effector-react": "^22.3.3",
    "ethers": "^5.7.1",
    "events": "^1.0.0",
    "gqty": "^2.3.0",
    "graphql": "^16.6.0",
    "moralis": "^2.6.5",
    "node-libs-browser": "^2.2.1",
    "react": "18.1.0",
    "react-native": "0.70.2",
    "react-native-crypto": "^2.2.0",
    "react-native-get-random-values": "^1.8.0",
    "react-native-randombytes": "^3.0.0",
    "react-native-reanimated": "^2.11.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "readable-stream": "1.0.33",
    "recompose": "^0.30.0",
    "rn-nodeify": "^10.3.0",
    "stream-browserify": "^1.0.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@gqty/cli": "^3.3.0-alpha-d8cdbf6.0",
    "@react-native-community/eslint-config": "^3.1.0",
    "@tsconfig/react-native": "^2.0.2",

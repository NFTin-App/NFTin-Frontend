{
    "name": "nftin",
    "version": "0.0.1",
    "private": true,
    "engines": {
        "node": ">=16"
    },
    "scripts": {
        "start": "react-native start",
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "types-check": "tsc --project tsconfig.json --noEmit --noEmitHelpers --noErrorTruncation --strict",
        "lint": "eslint src --ext .js,.jsx,.ts,.tsx,.json",
        "lint:fix": "yarn lint --fix",
        "lint:ci": "yarn lint --quiet",
        "precommit": "lint-staged",
        "release": "np --no-publish --no-tests --no-release-draft",
        "generate:graphql": "gqty generate",
        "generate:nftin-contract": "typechain --target=ethers-v5 'src/shared/api/nftinContract/config/*.json' --out-dir 'src/shared/api/nftinContract/generated'"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn precommit"
        }
    },
    "eslintConfig": {
        "extends": [
            "./configs/eslint.config"
        ]
    },
    "dependencies": {
        "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
        "@ethersproject/shims": "^5.7.0",
        "@react-native-async-storage/async-storage": "^1.17.10",
        "@react-navigation/bottom-tabs": "^6.4.0",
        "@react-navigation/native": "^6.0.13",
        "@react-navigation/native-stack": "^6.9.1",
        "@walletconnect/react-native-dapp": "^1.8.0",
        "@walletconnect/web3-provider": "^1.8.0",
        "big-integer": "^1.6.51",
        "buffer": "^6.0.3",
        "effector": "^22.3.0",
        "effector-react": "^22.3.3",
        "ethers": "^5.7.1",
        "events": "^1.0.0",
        "gqty": "^2.3.0",
        "graphql": "^16.6.0",
        "lodash": "^4.17.21",
        "moralis": "^2.6.5",
        "node-libs-browser": "^2.2.1",
        "patronum": "^1.12.3",
        "react": "18.1.0",
        "react-native": "0.70.2",
        "react-native-crypto": "^2.2.0",
        "react-native-drop-shadow": "^0.0.5",
        "react-native-get-random-values": "^1.8.0",
        "react-native-randombytes": "^3.6.1",
        "react-native-reanimated": "^2.11.0",
        "react-native-safe-area-context": "^4.4.1",
        "react-native-screens": "^3.18.2",
        "react-native-svg": "9.6.4",
        "react-native-video": "^5.2.1",
        "readable-stream": "1.0.33",
        "recompose": "^0.30.0",
        "typechain": "^8.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/runtime": "^7.12.5",
        "@gqty/cli": "^3.3.0-alpha-d8cdbf6.0",
        "@react-native-community/eslint-config": "^3.1.0",
        "@tsconfig/react-native": "^2.0.2",
        "@typechain/ethers-v5": "^10.1.0",
        "@types/lodash": "^4.14.186",
        "@types/react-native": "^0.70.4",
        "@types/react-native-video": "^5.0.14",
        "@types/react-test-renderer": "^18.0.0",
        "@types/recompose": "^0.30.10",
        "@typescript-eslint/eslint-plugin": "^5.40.0",
        "@typescript-eslint/parser": "^5.40.0",
        "babel-plugin-module-resolver": "^4.1.0",
        "babel-plugin-react-native-platform-specific-extensions": "^1.1.1",
        "cross-env": "^7.0.3",
        "eslint": "^8.25.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard-with-typescript": "^23.0.0",
        "eslint-plugin-effector": "^0.10.3",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-n": "^15.3.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.0.1",
        "eslint-plugin-react": "^7.31.10",
        "eslint-plugin-simple-import-sort": "^8.0.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "metro-react-native-babel-preset": "0.72.3",
        "prettier": "^2.7.1",
        "react-test-renderer": "18.1.0",
        "typescript": "^4.8.3"
    }
}
